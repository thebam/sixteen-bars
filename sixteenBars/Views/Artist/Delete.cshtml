@model sixteenBars.Library.Artist

<h1>Delete Artist</h1>
<h2>@ViewBag.ErrorMessage</h2>
@if (Model != null)
{
    <h3>Are you sure you want to delete "@Model.Name"?</h3>

    using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <p>
            <input type="submit" value="Delete" class="btn" />
            &nbsp;
            @Html.ActionLink("Cancel", "Index","Artist",null, new { @class = "btn btn-inverse" })
        </p>
    }

}
else
{
    <p>No Artist Found</p>
}