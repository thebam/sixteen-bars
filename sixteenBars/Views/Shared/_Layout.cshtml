</head>



<body>
    <header>
        <nav class="navbar navbar-inverse">
            <div class="container">
                
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink(" ", "Index", "Home", null, new { @class="logo"})
                </div>
                <div id="navbar" class="collapse navbar-collapse">
                    <ul id="nav-items" class="nav navbar-nav">
                        <li id="quote-nav-item">@Html.ActionLink("Quotes", "Index", "Quote")</li>
                        <li id="artist-nav-item">@Html.ActionLink("Artists", "Index", "Artist")</li>
                        <li id="album-nav-item">@Html.ActionLink("Albums", "Index", "Album")</li>
                        <li id="track-nav-item">@Html.ActionLink("Tracks", "Index", "Track")</li>
                    </ul>
                    <div class="btn-group explicit-toggle pull-right" role="group" aria-label="...">
                        <div id="explicit" class="language btn">
                            explicit
                        </div>
                        <div id="clean" class="language btn">
                            clean
                        </div>
                    </div>
                        <div id="searchApp" ng-app="SearchApp">
                            <div ng-controller="SearchController">
                                <div class="navbar-form col-sm-12 col-md-3 pull-right" role="search">
                                    <div class="input-group">
                                        <input type="text" class="form-control" placeholder="Search" ng-model="searchTerm" name="srch-term" id="srch-term">
                                        <span class="ng-cloak" ng-cloak style="color:red" ng-show="myForm.searchTerm.$dirty&&myForm.searchTerm.$isvalid">
                                            <span ng-show="myForm.searchTerm.$error.required">Search term is required</span>
                                        </span>
                                        <div class="input-group-btn">
                                            <button class="btn btn-default" ng-click="search()" type="submit"><i class="glyphicon glyphicon-search"></i></button>
                                        </div>
                                    </div>
                                </div>
                                <div id="searchResults" class="main-search-results col-md-12 col-sm-12 pull-right">
                                    <h4 class="ng-cloak" ng-cloak ng-show="searchtermTitle">Search Results For '{{searchTerm | uppercase}}'</h4>
                                    <p class="ng-cloak" ng-cloak ng-show="searchNoResults">No Results Found</p>
                                    <ul class="ng-cloak" ng-cloak>
                                        <li ng-repeat="result in results">
                                            {{result.Text}}  <a href="/{{result.URL}}">details</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                        </div>

                    </div><!--/.nav-collapse -->
            </div>
        </nav>
    </header>
    <div class="container">
        @RenderSection("featured", required: false)
        
        <div class="main-content">
            @RenderBody()
        </div>
    </div>
    <footer>
        
        <div class="container">
            <div class="login-container">
                @Html.Partial("_LoginPartial")
            </div>
            <p>&copy; @DateTime.Now.Year</p>
            
        </div>
        
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @Scripts.Render("~/Scripts/sixteenbars-utilities.js")
    @Scripts.Render("~/Scripts/angular.min.js")
    @Scripts.Render("~/Scripts/sixteenBars-search.js")

    
    

    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-563e10cde2a16f18" async="async"></script>
